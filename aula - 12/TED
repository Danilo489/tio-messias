from random import randint

A = []
B = []

for linha in range(10):
    linha = []

    for coluna in range(10):
        linha.append(randint(0, 10000))

    A.append(linha)

for linha_matriz in A:
    print(linha_matriz)

for linha in A:
    soma = sum(linha)
    print(soma)
    B.append(soma * 3)

print(B)
